#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/web/aula11/node_modules/.pnpm/eslint@8.41.0/node_modules/eslint/bin/node_modules:/mnt/c/web/aula11/node_modules/.pnpm/eslint@8.41.0/node_modules/eslint/node_modules:/mnt/c/web/aula11/node_modules/.pnpm/eslint@8.41.0/node_modules:/mnt/c/web/aula11/node_modules/.pnpm/node_modules:/mnt/c/web/aula11/node_modules:/mnt/c/web/node_modules:/mnt/c/node_modules:/mnt/c/web/aula11/node_modules/eslint/bin/node_modules:/mnt/c/web/aula11/node_modules/eslint/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/web/aula11/node_modules/.pnpm/eslint@8.41.0/node_modules/eslint/bin/node_modules:/mnt/c/web/aula11/node_modules/.pnpm/eslint@8.41.0/node_modules/eslint/node_modules:/mnt/c/web/aula11/node_modules/.pnpm/eslint@8.41.0/node_modules:/mnt/c/web/aula11/node_modules/.pnpm/node_modules:/mnt/c/web/aula11/node_modules:/mnt/c/web/node_modules:/mnt/c/node_modules:/mnt/c/web/aula11/node_modules/eslint/bin/node_modules:/mnt/c/web/aula11/node_modules/eslint/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../eslint/bin/eslint.js" "$@"
fi
